{
  "printWidth": 120,
  "singleQuote": true,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "semi": true,
  "arrowParens": "avoid",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "tailwindAttributes": [
    "className"
  ],
  "tailwindFunctions": [
    "clsx"
  ]
}